<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/tornado/cronjob.xsd">
    <import>
        <cronjob name="de.cryptonica.oauth2server.TokenCleanupCronjob">
            <classname>wcf\system\oauth2server\TokenCleanupCronjob</classname>
            <description>Entfernt die abgelaufenen OAuth2 Tokens</description>
            <startminute>*/5</startminute>
            <starthour>*</starthour>
            <startdom>*</startdom>
            <startmonth>*</startmonth>
            <startdow>*</startdow>
            <canbeedited>1</canbeedited>
            <canbedisabled>1</canbedisabled>
        </cronjob>
    </import>
</data>